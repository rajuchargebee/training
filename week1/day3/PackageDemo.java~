package pack1;
public class PackageDemo
{
	public float addition(float a,float b)
	{
		return (a+b);
	}
	public float subtraction(float a,float b)
	{
		return (a-b);
	}
	public float multiplication(float a,float b)
	{
		return (a*b);
	}
	public float devision(float a,float b)
	{
		return (a/b);
	}
}
